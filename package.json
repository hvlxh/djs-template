{
  "name": "template",
  "version": "1.0.0",
  "description": "A advanced template with eslint + prettier",
  "main": "source/app.js",
  "scripts": {
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "npm run lint:check && npm run format:check",
    "build:fix": "npm run lint:fix && npm run format:fix",
    "dev": "nodemon .",
    "test": "npm run build:fix"
  },
  "keywords": [],
  "author": "hvlxh",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "chalk": "^5.3.0",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1"
  }
}
